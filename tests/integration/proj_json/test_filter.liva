main() {
    let json_str = "[10, 25, 30, 15, 50, 5]"
    let numbers: [i32], err = JSON.parse(json_str)
    
    print($"Test 6: typed JSON parsing")
    print($"Original:")
    numbers.forEach(n => print($"  {n}"))
    
    // Note: filter removed temporarily due to codegen issue with parvec
    print($"Mapped (*2):")
    let doubled = numbers.map(n => n * 2)
    doubled.forEach(n => print($"  {n}"))
}
