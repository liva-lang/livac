// Test 5: map on JSON arrays with typed parsing
main() {
    let json_str = "[1, 2, 3, 4, 5]"
    let numbers: [i32], err = JSON.parse(json_str)
    
    print($"Test 5: map on JSON with typed parsing")
    let doubled = numbers.map(n => n * 2)
    
    print($"Original:")
    numbers.forEach(n => print($"  {n}"))
    
    print($"Doubled:")
    doubled.forEach(n => print($"  {n}"))
}
