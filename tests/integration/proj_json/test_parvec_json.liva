// Test 9: parvec (parallel execution) on JSON arrays with typed parsing
main() {
    let json_str = "[1, 2, 3, 4, 5, 6, 7, 8]"
    let numbers: [i32], err = JSON.parse(json_str)
    
    print($"Test 9: parvec on typed JSON")
    let doubled = numbers.parvec().map(n => n * 2)
    
    print($"Results (parallel):")
    doubled.forEach(n => print($"  {n}"))
}
