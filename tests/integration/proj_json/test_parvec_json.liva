// Test 9: parvec (parallel execution) on JSON arrays
main() {
    let json_str = "[1, 2, 3, 4, 5, 6, 7, 8]"
    let numbers = JSON.parse(json_str)
    
    print($"Test 9: parvec on JSON")
    let doubled = numbers.parvec().map(n => n.as_i32().unwrap() * 2)
    
    print($"Results (parallel):")
    doubled.forEach(n => print($"  {n}"))
}
