// Operadores aritméticos y lógicos
let sum = a + b
let diff = a - b
let prod = a * b
let div = a / b
let mod = a % b

// Operadores de comparación
let eq = a == b
let ne = a != b
let lt = a < b
let le = a <= b
let gt = a > b
let ge = a >= b

// Operadores lógicos (palabras)
let and_op = a and b
let or_op = a or b
let not_op = not a

// Operadores lógicos (símbolos)
let and_sym = a && b
let or_sym = a || b
let not_sym = !a
