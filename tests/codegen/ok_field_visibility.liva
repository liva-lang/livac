// Test for field visibility (private fields should be private in Rust)
User {
  name: string      // public
  _email: string    // private
  __password: string // private

  constructor(name, email, password) {
    this.name = name
    this._email = email
    this.__password = password
  }

  getName() => this.name
  getEmail() => this._email
  getPassword() => this.__password
}

main() {
  let user = User("John", "john@example.com", "secret123")

  print(user.getName())
  print(user.getEmail())
  print(user.getPassword())
}
