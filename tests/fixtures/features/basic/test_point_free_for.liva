// Phase 11.3 - Point-free function references in for loops

showItem(n: number) {
    print($"Item: {n}")
}

main() {
    let items = [1, 2, 3, 4, 5]

    // Point-free in for loop with print
    for item in items => print

    let items2 = [10, 20, 30]

    // Point-free in for loop with user function
    for item in items2 => showItem
}
