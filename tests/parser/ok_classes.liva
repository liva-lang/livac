Persona {
    nombre: string
    _edad: number
    __dni: string
    
    constructor(nombre: string, edad: number) {
        this.nombre = nombre
        this._edad = edad
    }
    
    getNombre(): string = this.nombre
    
    _getEdad(): number {
        return this._edad
    }
    
    __getDni(): string {
        return this.__dni
    }

    getEdadDNI():string = $"{this._getEdad()} {this.__getDni()}"
}

