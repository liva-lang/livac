// Visibilidad de miembros
Persona {
    nombre: string   // public
    _dni: string     // private
    
    getNombre(): string = this.nombre
    
    _getDni(): string {
        return this._dni
    }
}

main() {
    let p = Persona()
    let nombre = p.getNombre()  // OK: public method
    // let dni = p._getDni()     // Error: private method
}
