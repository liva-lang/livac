// Visibilidad de miembros
Persona {
    nombre: string           // public
    _edad: number           // protected
    __dni: string           // private
    
    getNombre(): string = this.nombre
    
    _getEdad(): number {
        return this._edad
    }
    
    __getDni(): string {
        return this.__dni
    }
}

Empleado extends Persona {
    salario: number
    
    getSalario(): number = this.salario
    
    // Puede acceder a _edad (protected)
    getEdadEmpleado(): number = this._getEdad()
}
