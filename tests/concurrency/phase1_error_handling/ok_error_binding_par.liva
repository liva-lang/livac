// Test: Error binding con par
// Expected: Compilar correctamente y manejar error

compute(n: number): number {
    if n < 0 fail "Negative number"
    return n * n
}

main() {
    // Error binding con parallel - caso de error
    let result, err = par compute(-5)
    
    if err != "" {
        print($"Error: {err}")
    } else {
        print($"Result: {result}")
    }
    
    // Error binding con parallel - caso de Ã©xito
    let result2, err2 = par compute(5)
    
    if err2 != "" {
        print($"Error: {err2}")
    } else {
        print($"Result: {result2}")
    }
}
