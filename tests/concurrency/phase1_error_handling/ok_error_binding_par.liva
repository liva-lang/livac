// Test: Error binding con par
// Expected: Compilar correctamente y manejar error

compute(n: number): Result<number, string> {
    if n < 0 {
        return Err("Negative number")
    }
    return Ok(n * n)
}

main() {
    // Error binding con parallel
    let result, err = par compute(-5)
    
    if err {
        print($"Error: {err}")
        return
    }
    
    print($"Result: {result}")
}
