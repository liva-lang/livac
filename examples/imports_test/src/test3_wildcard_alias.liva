import * as mathlib from "./lib/math.liva"
import * as strlib from "./lib/strings.liva"

main() {
    print("=== Test 3: Wildcard Imports with Alias ===")
    print("")
    
    // Access functions via alias
    print("Math via 'mathlib' alias:")
    print($"  mathlib.add(10, 20) = {mathlib.add(10, 20)}")
    print($"  mathlib.multiply(3, 7) = {mathlib.multiply(3, 7)}")
    print($"  mathlib.square(4) = {mathlib.square(4)}")
    print($"  mathlib.cube(3) = {mathlib.cube(3)}")
    print($"  mathlib.power(2, 8) = {mathlib.power(2, 8)}")
    print("")
    
    print("Strings via 'strlib' alias:")
    print($"  strlib.concat('foo', 'bar') = '{strlib.concat('foo', 'bar')}'")
    print($"  strlib.repeat('x', 5) = '{strlib.repeat('x', 5)}'")
    print($"  strlib.isEmpty('') = {strlib.isEmpty('')}")
    print($"  strlib.wrap('text', '[', ']') = '{strlib.wrap('text', '[', ']')}'")
    
    print("")
    print("âœ… Test 3 PASSED: Wildcard imports with alias work!")
}
