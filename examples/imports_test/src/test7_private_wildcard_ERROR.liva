// ============================================================================
// Test 7: Private via Wildcard (SHOULD NOT expose private symbols)
// ============================================================================
// When using wildcard import, private symbols should NOT be accessible

import * as math from "./lib/math.liva"

main() {
    print("=== Test 7: Private via Wildcard ===")
    
    // Public function - should work
    print($"math.add(1, 2) = {math.add(1, 2)}")
    
    // Private function - should NOT work via wildcard either!
    print($"math._power(2, 3) = {math._power(2, 3)}")
    
    print("‚ùå FAILED: Compiler should have rejected access to private via alias!")
}
