    nums.push(1)
    nums.push(2)
    nums.push(3)
    print($"Nums: {nums}")
    
    // This should generate: let mut strings = vec![];
    let strings: [string] = []
    strings.push("a")
    strings.push("b")
    print($"Strings: {strings}")
    
    print("âœ… Bug #43 test passed!")
