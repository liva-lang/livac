// Test Bug #43: mut inference with generic Stack
// This was the original failing case

Stack<T> {
    items: [T]
    
    constructor() {
        this.items = []
    }
    
    push(item: T) {
        this.items.push(item)
    }
    
    size(): number {
        return this.items.length
    }
}

main() {
    // This should generate: let mut num_stack = Stack::new();
    let numStack = Stack()
    numStack.push(10)
    numStack.push(20)
    numStack.push(30)
    print($"Size: {numStack.size()}")
    
    print("âœ… Bug #43 generic test passed!")
}
