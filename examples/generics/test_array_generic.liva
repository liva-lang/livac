// Test: Basic generics - identity function
// The simplest possible generic function

// Identity function - returns whatever you give it
identity<T>(value: T): T => value

main() {
    // Test with number
    let num = identity(42)
    print($"Identity of 42: {num}")
    
    // Test with string  
    let str = identity("Hello")
    print($"Identity of Hello: {str}")
    
    // Test with boolean
    let flag = identity(true)
    print($"Identity of true: {flag}")
}
